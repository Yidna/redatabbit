{
  "name": "modern-backbone-starterkit",
  "version": "1.0.0",
  "description": "Lightweight starting point for a modern backbone web app.",
  "keywords": [
    "backbone",
    "starterkit",
    "frontend",
    "boilerplate",
    "es6",
    "webpack"
  ],
  "scripts": {
    "start": "serve --port 3000 -s public | npm run backend",
    "test": "jest",
    "dev": "node webpack/webpack-dev-server | npm run backend",
    "coverage": "npm run test -- --coverage",
    "lint": "eslint src",
    "build": "cross-env NODE_ENV=production webpack --config webpack/webpack.config",
    "prepush": "npm run lint",
    "update": "node src/app/components/netflix-scraper/NetflixScraper",
    "backend": "node -e \"require('./src/app/components/database/DatabaseManager').createREST()\""
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.hbs$": "<rootDir>/webpack/jestHandlebarsLoader.js"
    }
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "babel-jest": "^19.0.0",
    "concurrently": "^3.1.0",
    "eslint": "^3.18.0",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-config-airbnb-base": "^11.1.2",
    "eslint-import-resolver-webpack": "^0.8.4",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-no-unused-vars-rest": "^1.0.4",
    "jest-cli": "^18.1.0",
    "nodemon": "^1.14.11",
    "webpack-dev-server": "^2.11.1",
    "zombie": "^5.0.7"
  },
  "dependencies": {
    "all": "0.0.0",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-lodash": "^3.2.11",
    "babel-polyfill": "^6.20.0",
    "babel-preset-env": "^1.2.2",
    "babel-preset-stage-1": "^6.16.0",
    "backbone": "^1.3.3",
    "backbone.marionette": "^3.2.0",
    "cheerio": "latest",
    "clean-webpack-plugin": "^0.1.16",
    "copy-webpack-plugin": "^4.3.1",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.1",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^2.0.0-beta",
    "file-loader": "^0.10.0",
    "flexboxgrid": "^6.3.1",
    "handlebars": "^4.0.6",
    "handlebars-loader": "^1.4.0",
    "history": "3.0.0",
    "html-webpack-harddisk-plugin": "^0.1.0",
    "html-webpack-plugin": "^2.28.0",
    "husky": "^0.13.1",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^3.3.1",
    "json-loader": "^0.5.4",
    "lodash": "^4.17.4",
    "marked": "^0.3.9",
    "normalize.css": "^5.0.0",
    "postcss-cssnext": "^2.9.0",
    "postcss-import": "^9.1.0",
    "postcss-loader": "^1.2.2",
    "postcss-nested": "^1.0.0",
    "puppeteer": "^0.13.0",
    "raw-loader": "^0.5.1",
    "request": "latest",
    "serve": "^5.1.2",
    "sqlite3": "^3.1.13",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webpack-bundle-analyzer": "^2.9.2",
    "webpack-dotenv-plugin": "^2.0.0"
  }
}
